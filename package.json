  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@heroicons/react": "1.0.5",
    "@prisma/client": "3.5.0",
    "@tailwindcss/typography": "0.4.1",
    "autoprefixer": "10.x",
    "blitz": "0.43.0",
    "discord.js": "13.3.1",
    "discordx": "9.0.1",
    "final-form": "4.20.4",
    "moment": "2.29.1",
    "passport-discord": "0.1.4",
    "postcss": "8.x",
    "react": "18.0.0-alpha-5ca4b0433-20211020",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "react-markdown": "7.1.0",
    "tailwindcss": "2.x",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@types/moment": "2.13.0",
    "@types/passport-discord": "0.1.5",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.36",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "3.5.0",
    "pretty-quick": "3.1.2",
    "preview-email": "3.0.5",
    "prisma": "3.5.0",
    "typescript": "~4.4"
  },
  "private": true
}
